(set-logic QF_S)

(declare-fun a1 () String)
(declare-fun a2 () String)
(declare-fun a3 () String)
(declare-fun a4 () String)
(declare-fun a51 () String)
(declare-fun a52 () String)
(declare-fun a6 () String)
(declare-fun a7 () String)
(declare-fun a8 () String)
(declare-fun var_S () String)

(assert (= a1 "GETz"))
(assert (not (str.in.re a2 (str.to.re ".*z.*")))) 
(assert (not (str.in.re a2 (str.to.re ".*host:.*"))))
(assert (= a3 "z"))
(assert (not (str.in.re a4 (str.to.re ".*host:.*"))))
(assert (= a51 "host:"))
(assert (= a5 (str.++ a51 a52)))
(assert (= a6 (str.++ a1 a2)))
(assert (= a7 (str.++ a3 a4)))
(assert (= a8 (str.++ a6 a7)))
(assert (= var_S (str.++ a8 a5)))
(assert (= (str.len var_S) 620))

(check-sat)
